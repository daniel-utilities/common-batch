{
    "git.branchProtection": [
        "main"
    ],
    "git.openRepositoryInParentFolders": "always",
    "git.autofetch": "all",


    "files.eol": "\n",
    "files.encoding": "utf8",
    "[bat]": {
        "files.encoding": "cp437",
        "files.eol": "\r\n",
        "editor.renderWhitespace": "boundary"
    },
    "[cmd]": {
        "files.encoding": "cp437",
        "files.eol": "\r\n",
        "editor.renderWhitespace": "boundary"
    },
    "[inf]": {
        "files.encoding": "utf16le",
        "files.eol": "\r\n",
        "editor.renderWhitespace": "boundary"
    },
    "[reg]": {
        "files.encoding": "utf16le",
        "files.eol": "\r\n",
        "editor.renderWhitespace": "boundary"
    },
    "[admx]": {
        "files.encoding": "utf8",
        "files.eol": "\r\n"
    },
    "[adml]": {
        "files.encoding": "utf8",
        "files.eol": "\r\n"
    },
    "[adm]": {
        "files.encoding": "utf16le",
        "files.eol": "\r\n"
    },
    "[cmtx]": {
        "files.encoding": "utf8bom",
        "files.eol": "\r\n"
    },
    "[pol]": {
        "files.encoding": "utf16le",
        "files.eol": "\r\n"
    },
    "[makefile]": {
        "editor.renderWhitespace": "boundary",
        "editor.detectIndentation": false,
        "editor.insertSpaces": false
    },


    "shellcheck.exclude": [
        "2181"
    ],
    "shellcheck.useWorkspaceRootAsCwd": true,


	"makefile.configurations": [

	],
    "makefile.defaultLaunchConfiguration": null
}
